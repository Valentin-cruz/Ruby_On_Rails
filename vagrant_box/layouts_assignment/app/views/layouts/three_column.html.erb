<!DOCTYPE html>
<html>
    <head>
        <title>LayoutsAssignment</title>
        <%= csrf_meta_tags %>
        <%= csp_meta_tag %>

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

    <body>
    <div class="container">
    <div class="row">
        <div class ="col-md-6">
            <%= yield :left_column %>
        </div>
        <div class ="col-md-6">
            <%= yield :middle_column %>
        </div>
        <div class ="col-md-6">
            <%= yield :right_column %>
        </div>
        </div>
    </div>

<%= yield %>
</body>
</html>